{
  "name": "@userback/widget",
  "description": "Userback.io widget for Javascript and Typescript",
  "version": "0.2.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/widget.mjs",
      "require": "./dist/widget.cjs",
      "types": "./dist/widget.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/widget.mjs",
  "module": "dist/widget.mjs",
  "umd": "dist/widget.umd.js",
  "types": "dist/widget.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "rollup": "^2.79.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "userback"
  ],
  "license": "MIT",
  "homepage": "https://github.com/userback/widget-js/tree/develop/widget-js#readme",
  "bugs": {
    "url": "https://github.com/userback/widget-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userback/widget-js.git"
  },
  "typedoc": {
    "entryPoint": "widget.ts",
    "readmeFile": "README.md"
  },
  "packageManager": "yarn@3.2.2"
}
