{
  "name": "@userback/react",
  "description": "Userback.io widget for React",
  "version": "0.3.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.cjs",
      "types": "./dist/react.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/react.cjs",
  "module": "dist/react.mjs",
  "umd": "dist/react.umd.js",
  "types": "dist/react.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@userback/widget": "workspace:^"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "rollup": "^2.79.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "keywords": [
    "userback",
    "react"
  ],
  "homepage": "https://github.com/userback/widget-js/tree/develop/widget-react#readme",
  "bugs": {
    "url": "https://github.com/userback/widget-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userback/widget-js.git"
  },
  "license": "MIT",
  "typedoc": {
    "entryPoint": "react.tsx",
    "readmeFile": "README.md"
  },
  "packageManager": "yarn@3.2.2"
}
