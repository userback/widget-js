{
  "name": "@userback/vue",
  "description": "Userback.io widget for Vue3",
  "version": "0.3.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs",
      "types": "./dist/vue.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/vue.cjs",
  "module": "dist/vue.mjs",
  "umd": "dist/vue.umd.js",
  "types": "dist/vue.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@userback/widget": "workspace:^"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "rollup": "^2.79.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "keywords": [
    "userback",
    "vue",
    "vue3"
  ],
  "homepage": "https://github.com/userback/widget-js/tree/develop/widget-vue#readme",
  "bugs": {
    "url": "https://github.com/userback/widget-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userback/widget-js.git"
  },
  "license": "MIT",
  "typedoc": {
    "entryPoint": "vue.tsx",
    "readmeFile": "README.md"
  },
  "packageManager": "yarn@3.2.2"
}
